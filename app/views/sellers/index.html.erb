<p id="notice"><%= notice %></p>

<h1>Listing Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Selleradmin</th>
      <th>Name</th>
      <th>Summary</th>
      <th>Tel</th>
      <th>Address</th>
      <th>Logo</th>
      <th>Hint</th>
      <th>Location</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.selleradmin_id %></td>
        <td><%= seller.name %></td>
        <td><%= seller.summary %></td>
        <td><%= seller.tel %></td>
        <td><%= seller.address %></td>
        <td><%= seller.logo %></td>
        <td><%= seller.hint %></td>
        <td><%= seller.location %></td>
        <td><%= seller.status %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
