<p id="notice"><%= notice %></p>

<h1>Listing Indents</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Diliveryman</th>
      <th>Number</th>
      <th>Status</th>
      <th>Orderstatus</th>
      <th>Sum</th>
      <th>Bltime</th>
      <th>Finishtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indents.each do |indent| %>
      <tr>
        <td><%= indent.customer_id %></td>
        <td><%= indent.diliveryman_id %></td>
        <td><%= indent.number %></td>
        <td><%= indent.status %></td>
        <td><%= indent.orderstatus %></td>
        <td><%= indent.sum %></td>
        <td><%= indent.bltime %></td>
        <td><%= indent.finishtime %></td>
        <td><%= link_to 'Show', indent %></td>
        <td><%= link_to 'Edit', edit_indent_path(indent) %></td>
        <td><%= link_to 'Destroy', indent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indent', new_indent_path %>
