<p id="notice"><%= notice %></p>

<h1>Listing Product Productcla Refs</h1>

<table>
  <thead>
    <tr>
      <th>Productcla</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_productcla_refs.each do |product_productcla_ref| %>
      <tr>
        <td><%= product_productcla_ref.productcla_id %></td>
        <td><%= product_productcla_ref.product_id %></td>
        <td><%= link_to 'Show', product_productcla_ref %></td>
        <td><%= link_to 'Edit', edit_product_productcla_ref_path(product_productcla_ref) %></td>
        <td><%= link_to 'Destroy', product_productcla_ref, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

